<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite" verbose="5" thread-count="2" parallel="tests">
    <test name="RadioButtonYes">
        <groups>
            <run>
                <include name="CLICK">
                </include>
            </run>
        </groups>
        <classes>
            <class name="TestNG.SuiteSample.RadioButtonYes">
            </class>
        </classes>
    </test>
    <test name="RadioButtonImpressive">
        <groups>
            <run>
                <include name="TITLE">
                </include>
            </run>
        </groups>
        <classes>
            <class name="TestNG.SuiteSample.RadioButtonImpressive">
            </class>
        </classes>
    </test>
</suite>